Public Function Copia(Optional dato As LaborVO) As LaborVO

  Dim datoVOtmp As New LaborVO
  Dim datoVOtmpOriginal As LaborVO

  If IsNull(dato) Then
    dato = Me 'se copia a si mismo
  Endif
  datoVOtmp.idlabor = dato.idlabor
  datoVOtmp.textocorto = dato.textocorto
  datoVOtmp.precioportrabajo = dato.precioportrabajo
  datoVOtmp.preciopordia = dato.preciopordia
  datoVOtmp.precioporhora = dato.precioporhora
  datoVOtmp.idsubcategoria = dato.idsubcategoria
  datoVOtmp.descripcion = dato.descripcion
  Return datoVOtmp

End

Public Function ConvertirVOaColeccion(dato As LaborVO) As Collection

  Dim colecciontmp As New Collection

  colecciontmp.add(dato.idlabor, "idlabor")
  colecciontmp.add(dato.textocorto, "textocorto")
  colecciontmp.add(dato.precioportrabajo, "precioportrabajo")
  colecciontmp.add(dato.preciopordia, "preciopordia")
  colecciontmp.add(dato.precioporhora, "precioporhora")
  colecciontmp.add(dato.idsubcategoria, "idsubcategoria")
  colecciontmp.add(dato.descripcion, "descripcion")
  Return colecciontmp

End

Public Function ConvertirColeccionaVO(dato As Collection) As LaborVO

  Dim datoVOtmp As New LaborVO

  Try datoVOtmp.idlabor = dato["idlabor"]
  Try datoVOtmp.textocorto = dato["textocorto"]
  Try datoVOtmp.precioportrabajo = dato["precioportrabajo"]
  Try datoVOtmp.preciopordia = dato["preciopordia"]
  Try datoVOtmp.precioporhora = dato["precioporhora"]
  Try datoVOtmp.idsubcategoria = dato["idsubcategoria"]
  Try datoVOtmp.descripcion = dato["descripcion"]
  Return datoVOtmp

End
