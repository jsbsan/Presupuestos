  Endif
  datoVOtmp.idpresupuesto = dato.idpresupuesto
  datoVOtmp.idcliente = dato.idcliente
  datoVOtmp.idestado = dato.idestado
  datoVOtmp.claveasignada = dato.claveasignada
  datoVOtmp.valorcalculado = dato.valorcalculado
  datoVOtmp.valorasignado = dato.valorasignado
